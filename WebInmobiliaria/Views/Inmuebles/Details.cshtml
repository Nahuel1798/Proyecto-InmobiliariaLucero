@model Inmueble

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles Inmueble</h1>

<div class="card">
    <div class="card-body">
        <dl class="row">
            <dt class = "col-sm-3">
                @Html.DisplayNameFor(model => model.Direccion)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Direccion)
            </dd>
            <dt class = "col-sm-3">
                @Html.DisplayNameFor(model => model.Uso)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Uso)
            </dd>
            <dt class = "col-sm-3">
                @Html.DisplayNameFor(model => model.TipoInmueble)
            </dt>
            <dd class = "col-sm-9">
                @Html.DisplayFor(model => model.TipoInmueble)
            </dd>
            <dt class = "col-sm-3">
                @Html.DisplayNameFor(model => model.Ambientes)
            </dt>
            <dd class = "col-sm-9">
                @Html.DisplayFor(model => model.Ambientes)
            </dd>
            <dt class = "col-sm-3">
                @Html.DisplayNameFor(model => model.Latitud)
            </dt>
            <dd class = "col-sm-9">
                @Html.DisplayFor(model => model.Latitud)
            </dd>
            <dt class = "col-sm-3">
                @Html.DisplayNameFor(model => model.Longitud)
            </dt>
            <dd class = "col-sm-9">
                @Html.DisplayFor(model => model.Longitud)
            </dd>
            <dt class = "col-sm-3">
                @Html.DisplayNameFor(model => model.Precio)
            </dt>
            <dd class = "col-sm-9">
                @Html.DisplayFor(model => model.Precio)
            </dd>
            <dt class = "col-sm-3">
                @Html.DisplayNameFor(model => model.Estado)
            </dt>
            <dd class = "col-sm-9">
                @Html.DisplayFor(model => model.Estado)
            </dd>
            <dt class = "col-sm-3">
                @Html.DisplayNameFor(model => model.Propietario)
            </dt>
            <dd class = "col-sm-9">
                @Html.DisplayFor(model => model.Propietario.Id)
            </dd>
        </dl>
    <a asp-action="Index" class="btn btn-secondary">Volver</a>
    </div>
</div>

