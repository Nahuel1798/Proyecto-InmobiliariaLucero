@model Pago

@{
    ViewData["Title"] = "Details";
}

<h1>Detalle</h1>

<div>
    <h4>Pago</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaPago)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaPago)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Importe)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Importe)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NumeroPeriodo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NumeroPeriodo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Observaciones)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Observaciones)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Contrato)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Contrato.Id)
        </dd>
        @if (User.IsInRole("Administrador"))
        {
            <h5>Auditor√≠a</h5>
            <dl class="row">
                <dt class="col-sm-3">Registrado por</dt>
                <dd class="col-sm-9">@Model.UsuarioAlta?.Email</dd>

                @if (Model.UsuarioAnulacion != null)
                {
                    <dt class="col-sm-3">Anulado por</dt>
                    <dd class="col-sm-9">@Model.UsuarioAnulacion?.Email</dd>
                }
            </dl>
        }
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Editar</a> |
    <a asp-action="Index">Volver</a>
</div>
