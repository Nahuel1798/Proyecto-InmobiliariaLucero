@model Inquilino

@{
    ViewData["Title"] = "Detalles";
}

<h1 class="mb-4">Detalles del Inquilino</h1>

<div class="card shadow-sm mb-4">
    <div class="card-body">
        <dl class="row">
            <dt class="col-sm-3 fw-semibold text-muted">
                @Html.DisplayNameFor(model => model.Dni)
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.Dni)
            </dd>

            <dt class="col-sm-3 fw-semibold text-muted">
                @Html.DisplayNameFor(model => model.NombreCompleto)
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.NombreCompleto)
            </dd>

            <dt class="col-sm-3 fw-semibold text-muted">
                @Html.DisplayNameFor(model => model.Telefono)
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.Telefono)
            </dd>

            <dt class="col-sm-3 fw-semibold text-muted">
                @Html.DisplayNameFor(model => model.Email)
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.Email)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="Index" class="btn btn-outline-primary">Volver</a>
    </div>
</div>


